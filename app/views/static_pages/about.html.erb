<% provide(:title, 'About Us') %>
<script>

var districts = []
$.getJSON( "/texaszip.json", function( data ) {
  
  $.each( data, function( key, val ) {
    if (typeof districts[val.z] == "undefined") {
      districts[val.z] = new Array();
    }
    districts[val.z].push(val.d);
  });
 
});

</script>
